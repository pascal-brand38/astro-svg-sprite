{
  "name": "astro-svg-sprite",
  "version": "1.1.0",
  "description": "A zero-dependency Astro Integration that generates a `sprite.svg` from SVG files in your Astro project.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "clear": "rm -rf ./dist",
    "build": "npm run clear && node at-scripts/index.mjs create-pkg-name && node at-scripts/index.mjs build && tsc -p tsconfig.build.json && esbuild dist/index.js  --outdir=dist --minify --allow-overwrite"
  },
  "keywords": [
    "withastro",
    "astro-integration",
    "astro",
    "astro-component",
    "astro-sprite-component",
    "sprite",
    "genertate sprite.svg",
    "sprite svg",
    "svg-sprite",
    "SVG Sprite",
    "spriter"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ACP-CODE/astro-svg-sprite.git"
  },
  "author": "Junlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ACP-CODE/astro-svg-sprite/issues"
  },
  "homepage": "https://github.com/ACP-CODE/astro-svg-sprite#readme",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "astro": "^2.10.3",
    "esbuild": "^0.24.0",
    "typescript": "^5.2.2"
  }
}
